# 本番環境設定
CONTAINER_PREFIX=superset-prod
COMPOSE_PROFILES=

# Superset設定
SUPERSET_PORT=8088
GUNICORN_WORKERS=4

# データベース接続設定（外部Azure PostgreSQL）
DB_DIALECT=postgresql+psycopg2
DB_USER=superset_admin
DB_PASSWORD=CHANGE_THIS_PASSWORD
DB_HOST=your-azure-postgres.postgres.database.azure.com
DB_PORT=5432
DB_NAME=superset

# Superset管理者設定
ADMIN_USERNAME=admin
ADMIN_FIRSTNAME=Admin
ADMIN_LASTNAME=User
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=CHANGE_THIS_ADMIN_PASSWORD

# セキュリティキー（本番環境では必ず変更すること）
SUPERSET_SECRET_KEY=CHANGE_THIS_SECRET_KEY_TO_RANDOM_STRING

# ローカルPostgreSQLは使用しない
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=
