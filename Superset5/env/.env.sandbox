# 開発サンドボックス環境設定
CONTAINER_PREFIX=superset-sandbox
COMPOSE_PROFILES=local

# Superset設定（ポート番号をデフォルト+1）
SUPERSET_PORT=8089
GUNICORN_WORKERS=2

# データベース接続設定（ローカルPostgreSQL）
DB_DIALECT=postgresql+psycopg2
DB_USER=superset
DB_PASSWORD=superset_sandbox_password
DB_HOST=superset-sandbox-postgres
DB_PORT=5433
DB_NAME=superset

# ローカルPostgreSQL設定
POSTGRES_DB=superset
POSTGRES_USER=superset
POSTGRES_PASSWORD=superset_sandbox_password

# Superset管理者設定
ADMIN_USERNAME=admin
ADMIN_FIRSTNAME=Admin
ADMIN_LASTNAME=Sandbox
ADMIN_EMAIL=admin@sandbox.local
ADMIN_PASSWORD=admin

# セキュリティキー（サンドボックス環境用）
SUPERSET_SECRET_KEY=sandbox_secret_key_change_in_production
