# 開発環境設定
CONTAINER_PREFIX=superset-dev
COMPOSE_PROFILES=local

# Superset設定
SUPERSET_PORT=8088
GUNICORN_WORKERS=2

# データベース接続設定（ローカルPostgreSQL）
DB_DIALECT=postgresql+psycopg2
DB_USER=superset
DB_PASSWORD=superset_dev_password
DB_HOST=superset-dev-postgres
DB_PORT=5432
DB_NAME=superset

# ローカルPostgreSQL設定
POSTGRES_DB=superset
POSTGRES_USER=superset
POSTGRES_PASSWORD=superset_dev_password

# Superset管理者設定
ADMIN_USERNAME=admin
ADMIN_FIRSTNAME=Admin
ADMIN_LASTNAME=Dev
ADMIN_EMAIL=admin@dev.local
ADMIN_PASSWORD=admin

# セキュリティキー（開発環境用）
SUPERSET_SECRET_KEY=dev_secret_key_change_in_production
